<AnchorPane fx:id="view" minHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.control.LogInController">
   <children>
      <AnchorPane onMouseClicked="#loguear" prefHeight="500.0" prefWidth="800.0">
         <children>
            <Label layoutX="264.0" layoutY="64.0" prefHeight="56.0" prefWidth="273.0" text="Inicio de sesión" textAlignment="CENTER">
               <font>
                  <Font size="39.0" />
               </font>
            </Label>
            <TextField fx:id="txtUser" layoutX="307.0" layoutY="199.0" />
            <TextField fx:id="txtPass" layoutX="307.0" layoutY="292.0" />
            <Label layoutX="359.0" layoutY="167.0" prefHeight="21.0" prefWidth="84.0" text="Usuario">
               <font>
                  <Font size="22.0" />
               </font>
            </Label>
            <Label layoutX="340.0" layoutY="260.0" prefHeight="32.0" prefWidth="120.0" text="Contraseña">
               <font>
                  <Font size="22.0" />
               </font>
            </Label>
            <Button fx:id="btnEntrar" layoutX="351.0" layoutY="372.0" mnemonicParsing="false" onAction="#loguear" prefHeight="40.0" prefWidth="100.0" text="Entrar" />
